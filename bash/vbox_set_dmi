#! /bin/bash
# This script sets the DMI information for VirtualBox.
VM_NAME="OfficeXP" # Name of your Virtual Machine
VSETED="VBoxManage setextradata $VM_NAME"
CFG_PATH="VBoxInternal/Devices/pcbios/0/Config"
$VSETED $CFG_PATH/DmiBIOSVendor       "FUJITSU // Phoenix Technologies Ltd."
$VSETED $CFG_PATH/DmiBIOSVersion      "Version 1.16"
$VSETED $CFG_PATH/DmiBIOSReleaseDate  "01/28/2008"
$VSETED $CFG_PATH/DmiBIOSReleaseMajor  2
$VSETED $CFG_PATH/DmiBIOSReleaseMinor  3
$VSETED $CFG_PATH/DmiBIOSFirmwareMajor 2
$VSETED $CFG_PATH/DmiBIOSFirmwareMinor 3
$VSETED $CFG_PATH/DmiSystemVendor     "FUJITSU"
$VSETED $CFG_PATH/DmiSystemProduct    "LifeBook T4215"
$VSETED $CFG_PATH/DmiSystemVersion    "<EMPTY>"
$VSETED $CFG_PATH/DmiSystemSerial     "R7102855"
$VSETED $CFG_PATH/DmiSystemUuid       "884E92A8-A4F6-11DB-8B14-001742269A00"
$VSETED $CFG_PATH/DmiSystemFamily     "X86-based PC"
